<p id="notice"><%= notice %></p>

<h1>Listing Admin Employees</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |admin_employee| %>
      <tr>
        <td><%= link_to 'Show', admin_employee %></td>
        <td><%= link_to 'Edit', edit_admin_employee_path(admin_employee) %></td>
        <td><%= link_to 'Destroy', admin_employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_admin_employee_path %>
